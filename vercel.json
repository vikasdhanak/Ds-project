{
  "version": 2,
  "builds": [
    {
      "src": "ebook-backend/src/server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "ebook-backend/src/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
